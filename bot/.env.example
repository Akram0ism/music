# Telegram bot token provided by BotFather
BOT_TOKEN=123456789:ABCDEFGHIJKLMNOPQRSTUVWXYZ

# Public URL for webhook deployment (e.g., Supabase Edge Function invoke URL)
WEBHOOK_URL=https://your-project.functions.supabase.co/telegram-webhook

# Optional secret token for validating webhook requests
WEBHOOK_SECRET_TOKEN=super-secret-token

# URL of the Telegram WebApp that should open from the /start command button
WEBAPP_URL=https://your-webapp.example.com

# Host and port for local development server
HOST=0.0.0.0
PORT=3000
